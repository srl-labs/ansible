# change this to the event type you want to get the data for
"on":
  release:
    types:
      - "created"
  push:

# enable this workflow to dump the full event json to the log
jobs:
  printJob:
    name: Print event
    runs-on: ubuntu-latest
    steps:
      - name: Dump GitHub context
        env:
          GITHUB_CONTEXT: ${{ toJson(github) }}
        run: |
          echo "$GITHUB_CONTEXT"
